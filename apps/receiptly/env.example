# Environment variables for Receiptly app

# Google Cloud Vision API (for OCR text recognition)
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Make sure to enable the Vision API in your Google Cloud project
EXPO_PUBLIC_GOOGLE_VISION_API_KEY=your_google_cloud_vision_api_key_here

# AWS Textract (fallback OCR service)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# Cash App Pay API (for payment integration)
CASH_APP_API_KEY=your_cash_app_api_key_here
CASH_APP_CLIENT_ID=your_cash_app_client_id_here 